<% @contributes.each do |contribute| %>
  <tr>
    <td><%= contribute.user.name %></td>
    <td><%= contribute.content %></td>
    <td><%= image_tag contribute.image if contribute.image.attached? %></td>
    <td>
    <% if contribute.status == 'accept' %>
     <%= button_tag contribute.status, type: "button", class: "btn btn-info status-contribute"%>
    <% else %>
      <%= button_tag contribute.status, type: "button", class: "btn btn-danger status-contribute"%>
    <% end %>
    </td>
    <td>
      <%= link_to admin_contribute_path(contribute) do %>
        <i class="fas fa-eye"></i>
      <% end %>
    </td>
  </tr>
<% end %>
